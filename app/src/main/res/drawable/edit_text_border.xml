<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
    <!-- Color de fondo -->
    <solid android:color="#043A27" />

    <!-- Borde verde como en la imagen -->
    <stroke
        android:width="3dp"
        android:color="#76D3A9" />

    <!-- Esquinas redondeadas -->
    <corners android:radius="16dp" />

    <!-- Un poco de padding interno -->
    <padding
        android:left="16dp"
        android:top="12dp"
        android:right="16dp"
        android:bottom="12dp" />
</shape>